number = {4894 Pt 1},
pages = {1027--33},
pmid = {2646709},
title = {{Genetic control of differentiation of the \emph{Caenorhabditis~elegans} touch receptor neurons.}},
% url = {http://www.ncbi.nlm.nih.gov/pubmed/2646709},
volume = {243},
