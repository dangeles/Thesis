journal = {Science},
keywords = {Animal,Animals,\emph{Caenorhabditis~elegans},\emph{Caenorhabditis~elegans}Proteins,\emph{Caenorhabditis~elegans}Proteins: metabolism,\emph{Caenorhabditis~elegans}: genetics,\emph{Caenorhabditis~elegans}: metabolism,Disease Models,Eicosanoids,Eicosanoids: metabolism,Evolution,Fatty Acids,Hypoxia-Inducible Factor 1,Hypoxia-Inducible Factor 1: metabolism,Molecular,Oxygen,Oxygen: metabolism,Reperfusion Injury,Reperfusion Injury: metabolism,Unsaturated,Unsaturated: metabolism},
number = {6145},
pages = {554--8},
