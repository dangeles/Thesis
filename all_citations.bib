title = {{Hypoxia and the HIF-1 transcriptional pathway reorganize a neuronal circuit for oxygen-dependent behavior in \emph{Caenorhabditis~elegans}}},
volume = {105},
year = {2008}
}
@article{Goentoro2009,
abstract = {In response to Wnt stimulation, ??-catenin accumulates and activates target genes. Using modeling and experimental analysis, we found that the level of ??-catenin is sensitive to perturbations in the pathway, such that cellular variation would be expected to alter the signaling outcome. One unusual parameter was robust: the fold-change in ??-catenin level (post-Wnt/pre-Wnt). In Xenopus, dorsal-anterior development and target gene expression are robust to perturbations that alter the final level but leave the fold-change intact. These suggest, first, that despite cellular noise, the cell responds reliably to Wnt stimulation by maintaining a robust fold-change in ??-catenin. Second, the transcriptional machinery downstream of the Wnt pathway does not simply read the ??-catenin level after Wnt stimulation but computes fold-changes in ??-catenin. Analogous to Weber's Law in sensory physiology, some gene transcription networks must respond to fold-changes in signals, rather than absolute levels, which may buffer stochastic, genetic, and environmental variation. ?? 2009 Elsevier Inc. All rights reserved.},
