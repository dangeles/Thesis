journal = {Molecular and cellular biology},
keywords = {Animals,Caenorhabditis elegans,Caenorhabditis elegans Proteins,Caenorhabditis elegans: cytology,Caenorhabditis elegans: embryology,Cell Division,Cell Survival,Helminth Proteins,Helminth Proteins: genetics,Helminth Proteins: physiology,Models, Biological,Mosaicism,ras Proteins,ras Proteins: physiology},
