issn = {1087-0156},
journal = {Nature biotechnology},
number = {5},
pages = {495--501},
pmid = {20436461},
publisher = {Nature Publishing Group},
title = {{GREAT improves functional interpretation of cis-regulatory regions.}},
volume = {28},
year = {2010}
}
