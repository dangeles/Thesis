@article{Sundaram2006,
abstract = {Receptor Tyrosine Kinase (RTK)/Ras GTPase/MAP kinase (MAPK) signaling pathways are used repeatedly during metazoan development to control many different biological processes. In the nematode  \emph{Caenorhabditis~elegans}  , two different RTKs (LET-23/EGFR and EGL-15/FGFR) are known to stimulate LET-60/Ras and a MAPK cascade consisting of the kinases LIN-45/Raf, MEK-2/MEK and MPK-1/ERK. This Ras/MAPK cascade is required for multiple developmental events, including induction of vulval, uterine, spicule, P12 and excretory duct cell fates, control of sex myoblast migration and axon guidance, and promotion of germline meiosis. Studies in \emph{C.~elegans}  have provided much insight into the basic framework of this RTK/Ras/MAPK signaling pathway, its regulation, how it elicits cell-type specific responses, and how it interacts with other signaling pathways such as the Wnt and Notch pathways.},
author = {Sundaram, Meera},
doi = {10.1895/wormbook.1.80.1},
isbn = {1551-8507 (Electronic)},
issn = {15518507},
