journal = {Developmental Biology},
keywords = {Animals,Cell Count,Cell Differentiation,Cell Division,Cell Nucleus,Cell Nucleus: ultrastructure,Cell Survival,Disorders of Sex Development,Embryonic Induction,Male,Nematoda,Nematoda: anatomy {\&} histology,Nematoda: physiology,Organ Specificity,Sex Differentiation,Species Specificity},
