keywords = {Animals,\emph{Caenorhabditis~elegans},\emph{Caenorhabditis~elegans}: physiology,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Life Expectancy,Longevity,Longevity: physiology,Survival Analysis,Survival Rate,Video Recording,Video Recording: methods},
number = {7},
pages = {665--70},
