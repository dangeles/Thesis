issn = {0219-1032},
journal = {Molecules and cells},
keywords = {Amino Acid Sequence,Animals,\emph{Caenorhabditis~elegans},\emph{Caenorhabditis~elegans}: physiology,Cell Nucleolus,Cell Nucleolus: enzymology,Conserved Sequence,GTP Phosphohydrolases,GTP Phosphohydrolases: physiology,Gene Expression,Heat-Shock Response,Insulin,Insulin: physiology,Lipid Metabolism,Longevity,Molecular Sequence Data,Protein Transport,Ribosomal Proteins,Ribosomal Proteins: metabolism,Somatomedins,Somatomedins: physiology,Transcriptional Activation},
number = {1},
pages = {51--7},
