issn = {00928674},
journal = {Cell},
number = {1},
pages = {109--126},
pmid = {10929718},
title = {{Functional Discovery via a Compendium of Expression Profiles}},
volume = {102},
year = {2000}
}
@article{Shen2006,
abstract = {Hypoxia-inducible factor (HIF) transcription factors implement essential changes in gene expression that enable animals to adapt to low oxygen (hypoxia). The stability of the  \emph{C.~elegans}HIF-1 protein is controlled by the evolutionarily conserved EGL-9/VHL-1 pathway for oxygen-dependent degradation. Here, we describe vhl-1-independent pathways that attenuate HIF-1 transcriptional activity in \emph{C.~elegans}. First, the expression of HIF-1 target genes is markedly higher in \emph{egl-9} mutants than in vhl-1 mutants. We show that HIF-1 protein levels are similar in animals carrying strong loss-of-function mutations in either \emph{egl-9} or vhl-1. We conclude that EGL-9 inhibits HIF-1 activity, as well as HIF-1 stability. Second, we identify the rhy-1 gene and show that it acts in a novel negative feedback loop to inhibit expression of HIF-1 target genes. rhy-1 encodes a multi-pass transmembrane protein. Although loss-of-function mutations in rhy-1 cause relatively modest increases in \emph{hif-1} mRNA and HIF-1 protein expression, some HIF-1 target genes are expressed at higher levels in rhy-1 mutants than in vhl-1 mutants. Animals lacking both vhl-1 and rhy-1 function have a more severe phenotype than either single mutant. Collectively, these data support models in which RHY-1 and EGL-9 function in VHL-1-independent pathway(s) to repress HIF-1 transcriptional activity.},
author = {Shen, Chuan and Shao, Zhiyong and Powell-Coffman, Jo Anne},
doi = {10.1534/genetics.106.063594},
file = {:Users/dangeles/Documents/Mendeley Library Copy/1205.full.pdf:pdf},
isbn = {0016-6731 (Print)$\backslash$n0016-6731 (Linking)},
issn = {00166731},
journal = {Genetics},
number = {3},
pages = {1205--1214},
pmid = {16980385},
title = {{The \emph{Caenorhabditis~elegans} \emph{rhy-1} Gene Inhibits HIF-1 Hypoxia-Inducible Factor Activity in a Negative Feedback Loop That Does Not Include \emph{vhl-1}}},
volume = {174},
year = {2006}
}
@article{Kaelin2007,
abstract = {Inactivation of the von Hippel-Lindau (VHL) tumor suppressor has been linked to a variety of tumors, including clear cell renal carcinoma, retinal and cerebellar hemangioblastoma, and pheochromocytoma. The best documented function of VHL protein (pVHL) relates to its ability to target the hypoxia-inducible transcription factor (HIF) for polyubiquitylation and proteasomal degradation. This chapter focuses on studies published over the past 2 years related to pVHL. These studies include those describing genetically engineered mice that were used to interrogate the relationship between pVHL and HIF in vivo and cell culture studies that underscore the importance of pVHL in epithelial differentiation and maintenance of the primary cilium. In addition, recent work suggests that pVHL regulates neuronal apoptosis in an HIF-independent manner, and this activity is linked to the risk of developing pheochromocytoma. ?? 2007 Elsevier Inc. All rights reserved.},
