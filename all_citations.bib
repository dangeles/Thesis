isbn = {1465-6914 (Electronic)$\backslash$n1465-6906 (Linking)},
issn = {1474-760X},
journal = {Genome biology},
keywords = {Animals,\emph{Caenorhabditis~elegans},\emph{Caenorhabditis~elegans}: genetics,\emph{Caenorhabditis~elegans}: metabolism,Computational Biology,Databases,Dimerization,Genes,Genetic,Genetic: genetics,Helminth,Helminth Proteins,Helminth Proteins: metabolism,Helminth: genetics,Humans,Open Reading Frames,Open Reading Frames: genetics,Promoter Regions,Protein Interaction Mapping,Protein Structure,RNA Splicing,RNA Splicing: genetics,Regulator,Regulator: genetics,Tertiary,Transcription,Transcription Factors,Transcription Factors: metabolism},
number = {13},
