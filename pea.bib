keywords = {Animals,Chromatin Immunoprecipitation,Data Mining,Data Mining: methods,Databases,E1A-Associated p300 Protein,Genetic,Genome,Genomics,Genomics: methods,Humans,Jurkat Cells,Mice,Protein Binding,Regulatory Elements,Serum Response Factor,Software,Transcriptional},
number = {5},
