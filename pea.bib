title = {{Mapping and quantifying mammalian transcriptomes by RNA-Seq}},
url = {http://dx.doi.org/10.1038/nmeth.1226{\%}5Cnhttp://www.nature.com/nmeth/journal/v5/n7/suppinfo/nmeth.1226{\_}S1.html{\%}5Cnhttp://www.nature.com/doifinder/10.1038/nmeth.1226{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/18516045},
