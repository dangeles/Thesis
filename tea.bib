keywords = {Avoidance behavior,Caenorhabditis elegans,Host-pathogen interactions},
number = {10},
pages = {465--470},
pmid = {25240986},
publisher = {Elsevier Ltd},
title = {{Behavioral avoidance of pathogenic bacteria by Caenorhabditis elegans}},
