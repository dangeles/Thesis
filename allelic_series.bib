issn = {0890-9369},
journal = {Genes {\&} development},
month = {jul},
number = {14},
pages = {1815--27},
pmid = {12130541},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{\emph{C.~elegans} EOR-1/PLZF and EOR-2 positively regulate Ras and Wnt signaling and function redundantly with LIN-25 and the SUR-2 Mediator component.}},
