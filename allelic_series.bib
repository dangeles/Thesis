doi = {10.1128/MCB.21.23.8104-8116.2001},
file = {:Users/dangeles/Documents/Mendeley Desktop/Schutzman et al. - 2001 - The  \emph{Caenorhabditis~elegans} EGL-15 signaling pathway implicates a DOS-like multisubstrate adaptor protein in(2).pdf:pdf},
issn = {0270-7306},
journal = {Molecular and cellular biology},
month = {dec},
number = {23},
pages = {8104--16},
pmid = {11689700},
publisher = {American Society for Microbiology},
title = {{The  \emph{Caenorhabditis~elegans} EGL-15 signaling pathway implicates a DOS-like multisubstrate adaptor protein in fibroblast growth factor signal transduction.}},
volume = {21},
year = {2001}
}

