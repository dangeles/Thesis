file = {:Users/dangeles/Documents/Mendeley Desktop/Yoon et al. - 1995 - Similarity of sli-1, a regulator of vulval development in \emph{C.~elegans} , to the mammalian proto-oncogene c-cbl.pdf:pdf},
issn = {0036-8075},
